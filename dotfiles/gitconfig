[user]
	name = pringshia
	email = pratik@ringshia.com
[alias]
	co = checkout
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  type = cat-file -t
  dump = cat-file -p
[color]
	diff = auto
	status = auto
	branch = auto
[core]
	excludesfile = ~/.gitignore
	editor = vim
	autocrlf = input
	pager = diff-so-fancy | less --tabs=4 -RFX
[apply]
	whitespace = nowarn
[format]
	pretty = %C(yellow)%h%Creset %s %C(red)(%cr, %aN)%Creset
[push]
	default=current
[credential]
	helper = osxkeychain
